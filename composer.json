{
    "name": "keyshang/laravel-authentication-log",
    "description": "Laravel Authentication Log provides authentication logger and notification for Laravel.",
    "keywords": [
        "laravel",
        "authentication",
        "log",
        "notification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Key Shang",
            "email": "arthur2sereno@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "KeyShang\\AuthenticationLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeyShang\\AuthenticationLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "KeyShang\\AuthenticationLog\\AuthenticationLogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
